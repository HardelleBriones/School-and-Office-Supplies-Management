        <condition/>
        <description>Hides cancel order button when in record creation form</description>
        <field/>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>Cancel Order Button display</name>
        <order/>
        <script><![CDATA[function onLoad() {
    // Check if the form is in add mode (record creation)
    if (g_form.isNewRecord()) {
        // Hide the button when creating a new record
        g_form.setDisplay('sys_ui_action_889dbf189794311015c7fbafe153af9f', false);
    } else {
        // Show the button for existing records
        g_form.setDisplay('sys_ui_action_889dbf189794311015c7fbafe153af9f', true);
    }
}]]></script>
